{
  "name": "tour-component",
  "version": "3.0.0",
  "description": "A vuejs, reactjs and angular tour component.",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc -p src && tsc -p demo",
    "tslint": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"demo/**/*.ts\" \"demo/**/*.tsx\"",
    "rev-static": "rev-static --config demo/rev-static.config.js",
    "lessc": "lessc src/tour.less > dist/tour.css",
    "stylelint": "stylelint \"src/**/*.less\"",
    "cleancss": "cleancss -o dist/tour.min.css dist/tour.css",
    "cleancss-demo": "cleancss -o demo/index.bundle.css dist/tour.min.css ./node_modules/github-fork-ribbon-css/gh-fork-ribbon.css",
    "clean": "rimraf dist",
    "clean-rev": "rimraf rimraf demo/**/index.bundle-*.js demo/*.bundle-*.css",
    "file2variable": "file2variable-cli src/vue.template.html -o src/vue-variables.ts --html-minify && file2variable-cli src/angular.template.html -o src/angular-variables.ts --html-minify",
    "build": "npm run clean-rev && npm run clean && npm run file2variable && npm run tsc && npm run lessc && npm run cleancss && npm run cleancss-demo && npm run webpack && npm run rev-static",
    "lint": "npm run tslint && npm run stylelint && npm run standard",
    "webpack": "webpack --config demo/webpack.config.js",
    "standard": "standard \"**/*.config.js\"",
    "fix": "standard --fix \"**/*.config.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/tour-component.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/tour-component/issues"
  },
  "homepage": "https://github.com/plantain-00/tour-component#readme",
  "devDependencies": {
    "@angular/common": "4.2.5",
    "@angular/compiler": "4.2.5",
    "@angular/core": "4.2.5",
    "@angular/forms": "4.2.5",
    "@angular/platform-browser": "4.2.5",
    "@angular/platform-browser-dynamic": "4.2.5",
    "@types/react": "15.0.34",
    "@types/react-dom": "15.5.1",
    "clean-css-cli": "4.1.6",
    "core-js": "2.4.1",
    "file2variable-cli": "1.3.0",
    "github-fork-ribbon-css": "0.2.1",
    "less": "2.7.2",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "rev-static": "2.5.0",
    "rimraf": "2.6.1",
    "rxjs": "5.4.1",
    "standard": "10.0.2",
    "stylelint": "7.12.0",
    "stylelint-config-standard": "16.0.0",
    "tslint": "5.4.3",
    "typescript": "2.4.1",
    "vue": "2.3.4",
    "vue-class-component": "5.0.2",
    "webpack": "3.0.0",
    "zone.js": "0.8.12"
  },
  "dependencies": {
    "tslib": "1.7.1"
  }
}
